{
  "Caption" : "Mirata Forms",
  "ActionBar": {
    "Items": [
      {
        "Fragment": "Generic",
        "Position": "left",
        "Text": "$(L,cancel)",
        "SystemItem": "/SAPAssetManager/Rules/UserPreferences/UserProfileCloseIcon.js",
        "OnPress": "/SAPAssetManager/Actions/Page/CancelPage.action",
        "_Type": "Control.Type.ActionBarItem"
      }
    ]
  },
  "Controls": [
    {
      "_Type": "Control.Type.SectionedTable",
      "_Name": "SectionedTable",
      "Sections": [
        {
          "_Type" : "Section.Type.ObjectTable",
          "Header": {
            "Caption": "Select the Mirata Form to use:",
            "UseTopPadding": false
          },
          "ObjectCell": {
            "Title": "{name}",
            "Subhead": "[{description}]",
            "AccessoryType": "disclosureIndicator",
            "OnPress": "/MirataFormsIntegrationComponents/Rules/Forms/Navigation/FormsExtensionPageNavFromFormsListViewPage.js",
            "_Type": "ObjectTable.Type.ObjectCell"
          },
          "Target": "/MirataFormsIntegrationComponents/Rules/Forms/FormsListView/GetFormInfoList.js"
        }
      ]
    }
  ],
  "_Type": "Page",
  "_Name": "FormsListView"
}
